{
  "version": "0.0.2",
  "manifest_version": 2,
  "name": "__webkitNotifications__",
  "description": "__webkitNotifications__",
  "default_locale": "zh_CN",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
   "background": {
    "scripts": ["js/socket.js", "js/index.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon.png",
      "38": "img/icon.png"
    },
    "default_title": "notification",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://*/",
    "tabs",  
    "notifications"  
  ]
}